{
  "name": "startsync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predev": "pnpm run typegen",
    "prebuild": "pnpm run typegen",
    "typegen": "sanity schema extract --path=./sanity/extract.json && sanity typegen generate"
